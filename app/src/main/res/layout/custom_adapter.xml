<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <imagelogoView
        android:id="@+id/imagelogo"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="26dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imagelogo"
        android:layout_marginStart="10dp"
        android:layout_toEndOf="@+id/imagelogo"
        android:text="TextView" />

    <TextView
        android:id="@+id/region"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imagelogo"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="7dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/timerange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/region"
        android:layout_alignBottom="@+id/region"
        android:layout_marginEnd="46dp"
        android:layout_toStartOf="@+id/region"
        android:text="TextView" />

    <TextView
        android:id="@+id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imagelogo"
        android:layout_alignStart="@+id/desc"
        android:text="TextView" />
</RelativeLayout>